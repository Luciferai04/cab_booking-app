{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "panels": [
    {
      "type": "timeseries",
      "title": "Latency by Route (p50/p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))",
          "legendFormat": "{{route}} p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, route))",
          "legendFormat": "{{route}} p95"
        }
      ],
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (5xx per minute)",
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_seconds_count{code=~\"5..\"}[5m])) by (route)",
          "legendFormat": "{{route}} 5xx/min"
        }
      ],
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 10 }
    },
    {
      "type": "timeseries",
      "title": "Redis Ops/sec",
      "targets": [
        { "expr": "rate(redis_commands_total[5m])", "legendFormat": "commands/s" },
        { "expr": "rate(redis_keyspace_hits_total[5m])", "legendFormat": "hits/s" },
        { "expr": "rate(redis_keyspace_misses_total[5m])", "legendFormat": "misses/s" }
      ],
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 20 }
    }
  ],
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["uber-video", "service"],
  "time": { "from": "now-1h", "to": "now" },
  "timezone": "browser",
  "title": "Service Latency & Errors & Redis",
  "version": 1
}

